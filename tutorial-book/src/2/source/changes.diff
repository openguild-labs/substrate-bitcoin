+			let difficulty = min(
+				U256::from(T::MaxDifficulty::get()),
+				max(
+					U256::from(T::MinDifficulty::get()),
+					diff_sum * U256::from(T::TargetBlockTime::get()) / U256::from(adj_ts),
+				),
+			);
+
+			<PastDifficultiesAndTimestamps<T, I>>::put(data);
+			<CurrentDifficulty<T, I>>::put(difficulty);
+		}
+	}
