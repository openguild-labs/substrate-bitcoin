+					Some(diff) => diff,
+					None => InitialDifficulty::<T, I>::get(),
+				};
+				diff_sum += diff;
+			}
+
+			if diff_sum < U256::from(T::MinDifficulty::get()) {
+				diff_sum = U256::from(T::MinDifficulty::get());
+			}
